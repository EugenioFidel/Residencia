	<parameter name="fechaInforme" class="java.util.Date">
		<parameterDescription><![CDATA[FechaReferenciaInforme]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT 
	residentor.persona.`letraCif`,
	residentor.persona.dni,
	residentor.persona.`letraNif`,
	residentor.persona.nombre,
	residentor.persona.`primerApe`,
	residentor.persona.`segundoApe`, 
	residentor.observacion.alimentacion,
	residentor.observacion.movilidad,
	residentor.observacion.aseo,
	residentor.observacion.vestido,
	residentor.observacion.inodoro,
	residentor.observacion.esfinteres,    
	residentor.observacion.`gradoDependencia`,
	max(residentor.observacion.`fechaObservacion`) 
FROM 
	residentor.persona,
	residentor.interno,
	residentor.observacion,
	residentor.interno_observacion
where
	 residentor.persona.`idPersona` = residentor.interno.`idPersona` 
and
	residentor.interno.`idPersona`=residentor.interno_observacion.`idPersona`
and
	  residentor.interno_observacion.`idObservacion` = residentor.observacion.`idObservacion` 
and
 	residentor.observacion.`fechaObservacion` <= $P{fechaInforme}
group by 
		residentor.persona.idPersona
order by
 residentor.persona.`primerApe` , residentor.persona.`segundoApe` , residentor.persona.nombre]]>
	</queryString>
